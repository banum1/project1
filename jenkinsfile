pipeline {
  agent any
  environment {
    PATH = "/opt/apache-maven-3.9.5/bin:${PATH}"
  }
  stages {
    stage("git clone code") {
      steps{
        git 'https://github.com/architectdevops7/jenkins-cicd.git'
      }
    }
    
    stage("maven build code"){
      steps{
        sh 'mvn clean package'
      }
    }
}
}
